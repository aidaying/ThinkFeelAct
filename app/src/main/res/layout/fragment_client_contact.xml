        android:layout_alignStart="@+id/editText" />

    <TextView
        android:text="Rainbow-In-Sight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="11dp"
        android:id="@+id/textView30"
        android:layout_above="@+id/editText3"
        android:layout_alignEnd="@+id/textView29" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/editText10"
        android:text="Sat Sun Mon   CLOSED"
        android:layout_below="@+id/editText9"
        android:layout_alignStart="@+id/editText6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/editText6"
        android:text="Thurs 8AM - 8PM"
        android:layout_below="@+id/editText7"
        android:layout_toEndOf="@+id/textView27" />

    <TextView
        android:text="Opening Hours"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView27"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/imageView3"
        android:layout_toStartOf="@+id/editText7" />

    <TextView
        android:text="Consultant:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp"
        android:id="@+id/textView9"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/imageView3" />

    <TextView
        android:text="Mobile:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:layout_below="@+id/textView9"
        android:layout_alignEnd="@+id/textView9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText3"
        android:text="Charles J Graty"
        android:layout_above="@+id/editText5"
        android:layout_toEndOf="@+id/textView26" />


</RelativeLayout>
