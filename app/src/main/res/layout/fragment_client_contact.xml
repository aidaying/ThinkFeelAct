<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/client_contact"
    tools:context="nz.ac.aut.rnd.team.thinkfeelactproject.timeoutmodel.Client_Contact"
    android:background="@android:color/transparent">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:id="@+id/ccLayout2"
        android:layout_below="@+id/ccLayout1"
        android:layout_height="50dp">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="100dp">

            <TextView
                android:text="Opening Hours"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:id="@+id/textView27"
                android:textAlignment="center"
                android:layout_weight="1"
                android:layout_width="170dp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="match_parent"
                            android:ems="10"
                            android:layout_weight="1"
                            android:id="@+id/editText7"
                            android:text="Tue 9AM - 4PM"
                            android:layout_height="10dp" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="1"
                            android:ems="10"
                            android:id="@+id/editText6"
                            android:text="Thurs 8AM - 8PM"/>

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="1"
                            android:ems="10"
                            android:id="@+id/editText9"
                            android:text="Fri 8AM - 5PM"/>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="1"
                            android:ems="10"
                            android:id="@+id/editText8"
                            android:text="Wed 8AM - 4PM"/>

                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:ems="10"
                    android:id="@+id/editText10"
                    android:text="Sat Sun Mon   CLOSED"
                    android:layout_height="30dp" />

            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:id="@+id/ccLayout1"
        android:layout_height="180dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <TextView
            android:text="Rainbow-In-Sight"
            android:layout_width="match_parent"
            android:id="@+id/textView30"
            android:layout_margin="10dp"
            android:textAlignment="center"
            android:textSize="12pt"
            android:layout_weight="1"
            android:layout_height="80dp" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <ImageView
                android:src="@drawable/charles"
                android:id="@+id/imageView3"
                android:adjustViewBounds="true"
                android:layout_height="match_parent"
                android:background="@android:color/transparent"
                android:cropToPadding="false"
                android:layout_weight="1"
                android:layout_width="150dp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:layout_height="250dp">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_height="match_parent"
                        android:layout_width="85dp">

                        <TextView
                            android:text="Consultant:"
                            android:layout_weight="1"
                            android:textAlignment="textEnd"
                            android:id="@+id/textView9"
                            android:layout_height="10dp"
                            android:layout_width="80dp" />

                        <TextView
                            android:text="Phone:"
                            android:layout_width="80dp"
                            android:textAlignment="textEnd"
                            android:layout_weight="1"
                            android:id="@+id/textView13"
                            android:layout_height="10dp" />

                        <TextView
                            android:text="Mobile:"
                            android:layout_width="80dp"
                            android:layout_height="10dp"
                            android:layout_weight="1"
                            android:id="@+id/textView12"
                            android:textAlignment="textEnd"/>

                        <TextView
                            android:text="Website:"
                            android:layout_height="10dp"
                            android:layout_weight="1"
                            android:id="@+id/textView25"
                            android:layout_width="80dp"
                            android:textAlignment="textEnd"/>

                        <TextView
                            android:text="Address:"
                            android:layout_height="10dp"
                            android:layout_weight="1"
                            android:id="@+id/textView26"
                            android:layout_width="80dp"
                            android:textAlignment="textEnd"/>

                        <TextView
                            android:layout_width="80dp"
                            android:layout_height="10dp"
                            android:id="@+id/textView33"
                            android:textAlignment="textEnd"
                            android:layout_weight="1" />

                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="1"
                            android:id="@+id/editText3"
                            android:text="Charles J Graty" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="1"
                            android:id="@+id/editText4"
                            android:text="+6421568170" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="1"
                            android:id="@+id/editText5"
                            android:text="+6498371301" />

                        <TextView
                            android:layout_height="10dp"
                            android:id="@+id/editText11"
                            android:layout_weight="1"
                            android:text="rainbow-in-sight.co.nz"
                            android:layout_width="match_parent" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="1"
                            android:id="@+id/editText"
                            android:text="1 Barsa Dr, Henderson" />

                        <TextView
                            android:text="Auckland 0612"
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="1"
                            android:id="@+id/textView29"/>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:id="@+id/ccLayout3"
        android:layout_below="@+id/ccLayout2"
        android:layout_height="350dp">

        <com.google.android.gms.maps.MapView
            android:layout_width="match_parent"
            android:id="@+id/mapView"
            android:layout_marginTop="5dp"
            android:layout_height="match_parent" />
    </LinearLayout>


</RelativeLayout>
